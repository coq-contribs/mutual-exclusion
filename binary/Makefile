VFILES:=
VOFILES:=$(VFILES:.v=.vo)
GLOBFILES:=$(VFILES:.v=.glob)
VIFILES:=$(VFILES:.v=.vi)
GFILES:=$(VFILES:.v=.g)
HTMLFILES:=$(VFILES:.v=.html)
GHTMLFILES:=$(VFILES:.v=.g.html)

all: version1\
  version2

###################
#                 #
# Subdirectories. #
#                 #
###################

version1:
	cd version1 ; $(MAKE) all

version2:
	cd version2 ; $(MAKE) all

####################
#                  #
# Special targets. #
#                  #
####################

.PHONY: all opt byte archclean clean install depend html version1 version2

byte:
	$(MAKE) all "OPT:=-byte"

opt:
	$(MAKE) all "OPT:=-opt"

install:
	mkdir -p `$(COQC) -where`/user-contrib
	(cd version1 ; $(MAKE) install)
	(cd version2 ; $(MAKE) install)

Makefile: Make
	mv -f Makefile Makefile.bak
	$(COQBIN)coq_makefile -f Make -o Makefile

	(cd version1 ; $(MAKE) Makefile)
	(cd version2 ; $(MAKE) Makefile)

clean:
	rm -f *.cmo *.cmi *.cmx *.o $(VOFILES) $(VIFILES) $(GFILES) *~
	rm -f all.ps all-gal.ps all.glob $(VFILES:.v=.glob) $(HTMLFILES) $(GHTMLFILES) $(VFILES:.v=.tex) $(VFILES:.v=.g.tex) $(VFILES:.v=.v.d)
	- rm -rf html
	(cd version1 ; $(MAKE) clean)
	(cd version2 ; $(MAKE) clean)

archclean:
	rm -f *.cmx *.o
	(cd version1 ; $(MAKE) archclean)
	(cd version2 ; $(MAKE) archclean)


